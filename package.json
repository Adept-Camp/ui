    "@web3-react/ledger-connector": "^6.0.2",
    "@web3-react/network-connector": "^6.0.2",
    "@web3-react/portis-connector": "^6.0.2",
    "@web3-react/squarelink-connector": "^6.0.2",
    "@web3-react/torus-connector": "^6.0.2",
    "@web3-react/trezor-connector": "^6.0.2",
