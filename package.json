    "bn.js": "^4.11.8",
    "browserslist": "^4.8.2",
    "bs-css": "^10.0.1",
    "bs-dynamic-import": "^0.4.0",
    "bs-fetch": "^0.5.0",
    "bs-moment": "https://github.com/reasonml-community/bs-moment.git#0d476e5",
    "copy-to-clipboard": "^3.2.0",
    "ethers": "^4.0.41",
    "moment": "^2.24.0",
    "multicodec": "0.5.6",
    "node-sass": "^4.13.0",
    "re-formality": "^3.1.0",
    "react": "^16.10.2",
    "react-countup": "^4.2.2",
    "react-dom": "^16.8.6",
    "react-scripts": "^3.3.0",
    "react-switch": "^5.0.1",
    "react-tabs": "^3.0.0",
    "react-translate": "^7.0.1",
    "reason-loadable": "^0.6.2",
    "reason-promise": "^1.0.0",
    "reason-react": "^0.7.0",
    "rimble-ui": "^0.10.0",
    "styled-components": "^4.3.2",
    "use-debounce": "^3.1.0"
  },
  "scripts": {
    "start": "npm run re:build && react-scripts start",
    "build": "npm run re:build && EXTEND_ESLINT=true react-scripts build",
    "test": "react-scripts test",
    "re:build": "bsb -make-world -clean-world",
    "re:watch": "bsb -make-world -clean-world -w",
    "deploy": "npm run build && gh-pages -d dist --remote JasoonS",
    "cra:deploy": "npm run re:build && npm run cra:build && gh-pages -d build --remote JasoonS",
    "generate-ppx-schema": "gqlschema https://api.thegraph.com/subgraphs/name/wild-cards/wildcards -i -o graphql_schema.json",
    "generate-ppx-schema-goerli": "gqlschema https://api.thegraph.com/subgraphs/name/wild-cards/wildcards-goerli -i -o graphql_schema.json"
  },
  "jest": {
    "transformIgnorePatterns": [
      "/node_modules/(?!@glennsl/bs-|bs-platform|bs-moment).+\\.js$"
    ]
  },
  "browserslist": {
    "production": [
      "> 1%",
      "last 2 versions",
      "not ie <= 8"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@glennsl/bs-jest": "^0.4.9",
    "@jsiebern/get_in_ppx": "^1.0.4",
    "@welldone-software/why-did-you-render": "^3.4.1",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "bs-let": "^0.1.11",
    "bs-platform": "^7.0.1",
    "gh-pages": "^2.0.1",
    "gql-tools": "0.0.15",
    "jest": "^24.9.0",
    "typescript": "^3.5.3"
  },
  "homepage": ".",
  "eslintConfig": {
    "extends": "react-app",
    "overrides": [
      {
        "files": [
          "**/*.js?(x)"
        ],
        "rules": {
          "react-hooks/rules-of-hooks": "warn"
        }
      }
    ]
  }
}
